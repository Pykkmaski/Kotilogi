{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "@/components/*": ["components/*"],
        "@/pages/*": ["pages/*"],
        "@/styles/*": ["styles/*"],
        "kotilogi-app/*": ["./*"],
        "@/*": ["./*"],
        "@/utils/*" : ["utils/*"],
      },
      "plugins": [
        {
          "name": "next"
        }
      ]
    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "types.d.ts",
      ".next/types/**/*.ts"
, "app/register/page.test.jsx", "__mocks__/dbconfig.js", "utils/__tests__/array.test.js", "app/api/auth/[...nextauth]/options.ts", "pelican/pelican.js", "app/api/auth/[...nextauth]/route.js", "dbconfig.js", "stories/Button.stories.js", "jest.setup.js", "jest.config.js", "utils/__tests__/properties.test.js"    ],
    "exclude": ["node_modules"],
    "types": ["@testing-library/jest-dom"],
  }